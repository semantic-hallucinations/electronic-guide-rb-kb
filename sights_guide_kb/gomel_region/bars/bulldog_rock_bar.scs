        [BullDog Rock бар]
        (* <- lang_ru;; *);
        [BullDog Rock bar]
        (* <- lang_en;; *);

    => nrel_age_restrictions:
        [18]
        (* <- concept_age;; *);

    => nrel_number_of_halls:
        [1]
        (* <- number;; *);
  
    => nrel_working_hours:
        [17:00 - 01:00]
        (* <- time;; *);

