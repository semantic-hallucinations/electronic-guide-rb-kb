la_salute_restaurant

    => nrel_main_idtf:
        [La Salute ресторан]
        (* <- lang_ru;; *);
        [La Salute restaurant]
        (* <- lang_en;; *);

    => nrel_age_restrictions:
        [0]
        (* <- concept_age;; *);

    => nrel_people_capacity:
        [58]
        (* <- number;; *);

    => nrel_working_hours:
        [10:30 - 23:00]
        (* <- time;; *);

    => nrel_adress:
	[Рогачёвская ул., 4] (* <- lang_ru;; *);
	[Rogachevskaya st., 4] (* <- lang_en;; *);

