puppet_theater_brest

    => nrel_main_idtf:
        [Брестский областной театр кукол]
        (* <- lang_ru;; *);
        [Brest Regional Puppet Theater]
        (* <- lang_en;; *);

    => nrel_age_restrictions:
        zero_plus;

    => nrel_people_capacity:
        [303]
        (* <- number;; *);

    => nrel_number_of_halls:
        [2]
        (* <- number;; *);

