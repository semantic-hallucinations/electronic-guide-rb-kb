jules_vern_restaraunt_brest

    => nrel_main_idtf:
        [Ресторан "Жюль Верн" в Бресте]
        (* <- lang_ru;; *);
        [Restaraunt "Tavern on Kovelskaya" in Brest]
        (* <- lang_en;; *);

    => nrel_age_restrictions:
        zero_plus;

    => nrel_people_capacity:
        [100]
        (* <- number;; *);

    => nrel_number_of_halls:
        [2]
        (* <- number;; *);
    
    => nrel_wifi_availability:
        wifi_available;  

    => nrel_accesible_environment:
        not_accesible;


    => nrel_working_hours:
        [12:00 - 23:00]
        (* <- time;; *); 

