embrace_restaraunt_brest

    => nrel_main_idtf:
        [Ресторан "EmbracE" в Бресте]
        (* <- lang_ru;; *);
        [Restaraunt "EmbracE" in Brest]
        (* <- lang_en;; *);

    => nrel_age_restrictions:
        zero_plus;
        
    => nrel_number_of_halls:
        [1]
        (* <- number;; *);
    
    => nrel_wifi_availability:
        wifi_available;  

    => nrel_accesible_environment:
        accessible;

    => nrel_working_hours:
        [12:00 - 23:00]
        (* <- time;; *); 

    <-concept_restaraunt;

    <= nrel_located_in: 
        brest_city;
    
    => nrel_sight_description:
        [Ресторан "EmbracE" здесь можно заказать столик для компании, забежать выпить кофе или пообедать. 
