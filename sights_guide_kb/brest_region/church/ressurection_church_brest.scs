        [Собор Воскресения Христова в Бресте]
        (* <- lang_ru;; *);
        [Cathedral of the Resurrection of Christ in Brest]
        (* <- lang_en;; *);

    => nrel_age_restrictions:
        zero_plus;

    => nrel_people_capacity:
        [5000]
        (* <- number;; *);

