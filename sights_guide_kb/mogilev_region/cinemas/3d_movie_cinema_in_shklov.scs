	concept_cinema; 

<= nrel_lockated_in:
	shklov_city;

=> nrel_age_restrictions:
        [0]
        (* <- concept_age;; *);
        
=> nrel_number_of_halls:
        [2]
        (* <- number;; *);

=> nrel_main_idtf:
    [Кинотеатр 3D кино]
    (* <- lang_ru;;*);
