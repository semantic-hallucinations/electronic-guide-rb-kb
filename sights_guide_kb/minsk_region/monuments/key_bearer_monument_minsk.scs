
key_bearer_monument_minsk

    => nrel_main_idtf:
        [Монумент "Войт с ключом" в Минске]
        (* <- lang_ru;; *);
        [Key Bearer Monument in Minsk]
        (* <- lang_en;; *);

    => nrel_working_hours:
        [Круглосуточно]
        (* <- lang_ru;; *);

    => nrel_accessible_environment:
        accessible;

    => nrel_wifi_availability:
        wifi_not_available;

    <= nrel_inclusion: 
        concept_monument;

    <= nrel_located_in: 
