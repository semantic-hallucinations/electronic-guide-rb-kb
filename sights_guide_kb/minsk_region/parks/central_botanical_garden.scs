
central_botanical_garden_minsk

    => nrel_main_idtf:
        [Центральный ботанический сад Национальной академии наук Беларуси в Минске]
        (* <- lang_ru;; *);
        [Central Botanical Garden of the National Academy of Sciences of Belarus in Minsk]
        (* <- lang_en;; *);

    => nrel_working_hours:
        [10:00 - 18:15]
        (* <- time;; *);

    => nrel_accessible_environment:
        accessible;

    => nrel_wifi_availability:
        wifi_not_available;

    <= nrel_inclusion: 
        concept_garden;

    <= nrel_located_in: 
