
holy_elena_church_minsk

    => nrel_main_idtf:
        [Храм Святой Елены в Минске]
        (* <- lang_ru;; *);
        [Holy Elena Church in Minsk]
        (* <- lang_en;; *);

    => nrel_working_hours:
        [08:00 - 18:00] 
        (* <- time;; *);
        [08:00 - 20:00]
        (* <- time;; *);

    <= nrel_inclusion: 
        concept_historical_sight;

    <= nrel_located_in: 
