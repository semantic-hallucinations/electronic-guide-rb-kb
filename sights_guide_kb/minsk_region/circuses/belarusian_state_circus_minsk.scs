belarusian_state_circus_minsk

    => nrel_main_idtf:
        [Белорусский государственный цирк в Минске]
        (* <- lang_ru;; *);
        [Belarusian State Circus in Minsk]
        (* <- lang_en;; *);

    => nrel_working_hours:
        [11:00 - 20:00]
        (* <- time;; *);
    
    => nrel_people_capacity:
        [1581]
        (* <- number;; *);

